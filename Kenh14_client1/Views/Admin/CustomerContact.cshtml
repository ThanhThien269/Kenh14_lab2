@model List<Kenh14_client1.Models.Customer>

<div class="main_container">
        <!-- page content -->
        <div class="right_col" role="main" style="min-height: 1180px;">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Customer Contacts</h3>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">
              <div class="col-md-12">
                <div class="x_panel">
                  <div class="x_content">
                    <div class="row">
                      <div class="clearfix"></div>

                      <div class="col-md-4 col-sm-4 col-xs-12 profile_details">
                        <div class="well profile_view">
                            <div class="col-sm-12">
                            <h4 class="brief"><i>Customer</i></h4>
                            @foreach(var item in Model)
                            {
                            <div class="left col-xs-7">
                              <h2>@item.Name</h2>
                              <p>@item.DateofBirth</p>
                              <ul class="list-unstyled">
                                <li><i class="fa fa-building"></i>@item.Address</li>
                                <li><i class="fa fa-phone"></i>@item.Telephone</li>
                              </ul>
                              <p>@item.Care</p>
                            </div>
                            }
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /page content -->
      </div>